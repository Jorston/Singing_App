<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <style android:name="buttonStyle">
        <item android:name="android:textColor">#ffffff</item>
        <item android:name="android:background">#80000000</item>
        <solid android:color="#000"/>
        <item android:name="android:button">
            <shape
                android:shape="rectangle" >
                <stroke
                    android:width="10dp"
                    android:color="#ffffff" />
            </shape>
        </item>

    </style>

</selector>